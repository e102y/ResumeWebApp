<html>
    <head>
        <title>Resumes</title>
    </head>
    <body>
        <h1>Resumes</h1>
        <a href="/resume/add">Add New Resume</a> <br />

        <br />
        <table border="1">
            <tr><th>Resume Name</th><th>User Name</th></tr>
            <% for(var i=0; result.length > i; i++ ) { %>
            <tr>
                <td><a href="/resume/?resume_id=<%= result[i].resume_id %>"><%= result[i].resume_name %></a></td>
                <td><%= result[i].first_name %> <%= result[i].last_name %></td>
            </tr>
            <% } %>
        </table>
    </body>

</html>